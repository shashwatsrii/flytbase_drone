# Lombok configuration to fix compatibility issues
lombok.addLombokGeneratedAnnotation = true
lombok.anyConstructor.addConstructorProperties = false
lombok.extern.findbugs.addSuppressFBWarnings = false

# Disable specific features that might cause compiler issues
lombok.accessors.chain = false
lombok.fieldDefaults.defaultPrivate = false
lombok.fieldDefaults.defaultFinal = false
lombok.equalsAndHashCode.callSuper = skip
lombok.toString.callSuper = skip

# Workaround for JDK 11 compatibility issues
lombok.equalsAndHashCode.doNotUseGetters = true
lombok.toString.doNotUseGetters = true
lombok.data.flagUsage = allow
lombok.value.flagUsage = allow
lombok.experimental.flagUsage = warning

# Use stable APIs only
config.stopBubbling = true
